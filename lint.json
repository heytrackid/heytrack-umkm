ation and log its performance\n   */\n  endOperation(operationId: string, context?: Record<string, any>): number | null {\n    const startTime = this.ongoingOperations.get(operationId)\n    if (startTime === undefined) {\n      return null\n    }\n\n    const duration = performance.now() - startTime\n    this.ongoingOperations.delete(operationId)\n\n    // Log slow operations\n    if (duration > 200) { // More than 200ms is considered slow\n      apiLogger.warn({ \n        operationId, \n        duration: duration.toFixed(2),\n        ...context \n      }, 'Slow operation detected')\n    } else if (duration > 50) {\n      apiLogger.info({ \n        operationId, \n        duration: duration.toFixed(2),\n        ...context \n      }, 'Operation completed')\n    }\n\n    return duration\n  }\n\n  /**\n   * Wrap an async function with performance monitoring\n   */\n  async measure<T>(operationId: string, fn: () => Promise<T>, context?: Record<string, any>): Promise<T> {\n    this.startOperation(operationId)\n    try {\n      const result = await fn()\n      this.endOperation(operationId, context)\n      return result\n    } catch (error) {\n      this.endOperation(operationId, { ...context, error: (error as Error).message })\n      throw error\n    }\n  }\n\n  /**\n   * Monitor memory usage and detect potential leaks\n   */\n  startMemoryMonitoring(intervalMs = 5000): void {\n    if (typeof window !== 'undefined' && 'memory' in performance) {\n      const memoryInterval = setInterval(() => {\n        const {memory} = (performance as any)\n        if (memory) {\n          const currentUsage = memory.usedJSHeapSize\n          \n          // Track memory usage history for leak detection\n          this.memoryUsageHistory.push({\n            timestamp: Date.now(),\n            used: currentUsage\n          })\n\n          // Keep only last 100 measurements\n          if (this.memoryUsageHistory.length > 100) {\n            this.memoryUsageHistory.shift()\n          }\n\n          // Calculate average memory usage over time\n          const avgUsage = this.memoryUsageHistory.reduce((sum, m) => sum + m.used, 0) / this.memoryUsageHistory.length\n\n          // Detect potential memory leaks (if memory usage is significantly above average)\n          if (currentUsage > avgUsage * 1.5 && this.memoryUsageHistory.length > 10) {\n            apiLogger.warn({\n              currentUsage,\n              avgUsage,\n              timestamp: Date.now()\n            }, 'Potential memory leak detected')\n          }\n\n          // Log memory usage for monitoring\n          apiLogger.debug({\n            used: memory.usedJSHeapSize,\n            total: memory.totalJSHeapSize,\n            limit: memory.jsHeapSizeLimit\n          }, 'Memory usage')\n        }\n      }, intervalMs)\n\n      this.memoryIntervals.add(memoryInterval)\n    }\n  }\n\n  /**\n   * Start monitoring Core Web Vitals\n   */\n  startWebVitalsMonitoring(): void {\n    if (typeof window === 'undefined' || !('PerformanceObserver' in window)) {\n      return\n    }\n\n    // Largest Contentful Paint (LCP)\n    try {\n      const lcpObserver = new PerformanceObserver((list) => {\n        const entries = list.getEntries()\n        const lastEntry = entries[entries.length - 1] as PerformanceEntry & { size?: number }\n\n        apiLogger.info({\n          type: 'LCP',\n          value: lastEntry.startTime,\n          size: lastEntry.size\n        }, 'Largest Contentful Paint')\n      })\n      lcpObserver.observe({ entryTypes: ['largest-contentful-paint'] })\n      this.observers.add(lcpObserver)\n    } catch (_e) {\n      // LCP observation not supported\n    }\n\n    // First Input Delay (FID)\n    try {\n      const fidObserver = new PerformanceObserver((list) => {\n        const entries = list.getEntries()\n        entries.forEach((entry: PerformanceEntry) => {\n          const firstInputEntry = entry as PerformanceEventTiming\n          const fid = firstInputEntry.processingStart - firstInputEntry.startTime\n          \n          if (fid > 100) { // More than 100ms is considered high FID\n            apiLogger.warn({\n              type: 'FID',\n              value: fid\n            }, 'High First Input Delay')\n          }\n        })\n      })\n      fidObserver.observe({ entryTypes: ['first-input'] })\n      this.observers.add(fidObserver)\n    } catch (_e) {\n      // FID observation not supported\n    }\n\n    // Cumulative Layout Shift (CLS)\n    try {\n      let clsValue = 0\n      const clsObserver = new PerformanceObserver((list) => {\n        const entries = list.getEntries()\n        entries.forEach((entry: PerformanceEntry) => {\n          const layoutShiftEntry = entry as any\n          if (!layoutShiftEntry.hadRecentInput) {\n            clsValue += layoutShiftEntry.value\n          }\n        })\n\n        if (clsValue > 0.1) { // More than 0.1 is considered high CLS\n          apiLogger.warn({\n            type: 'CLS',\n            value: clsValue\n          }, 'High Cumulative Layout Shift')\n        }\n      })\n      clsObserver.observe({ entryTypes: ['layout-shift'] })\n      this.observers.add(clsObserver)\n    } catch (_e) {\n      // CLS observation not supported\n    }\n\n    // First Contentful Paint (FCP)\n    try {\n      const fcpObserver = new PerformanceObserver((list) => {\n        const entries = list.getEntries()\n        entries.forEach((entry) => {\n          apiLogger.info({\n            type: 'FCP',\n            value: entry.startTime\n          }, 'First Contentful Paint')\n        })\n      })\n      fcpObserver.observe({ entryTypes: ['paint'] })\n      this.observers.add(fcpObserver)\n    } catch (_e) {\n      // FCP observation not supported\n    }\n  }\n\n  /**\n   * Add event listener with proper cleanup tracking\n   */\n  addTrackedEventListener<K extends keyof HTMLElementEventMap>(\n    element: HTMLElement | Window,\n    event: K,\n    handler: (this: HTMLElement, ev: HTMLElementEventMap[K]) => any\n  ): () => void {\n    const wrappedHandler = (ev: HTMLElementEventMap[K]) => {\n      const operationId = `event-${event}-${Date.now()}`\n      this.startOperation(operationId)\n      try {\n        return handler.call(element, ev)\n      } finally {\n        this.endOperation(operationId, { eventType: event })\n      }\n    }\n\n    element.addEventListener(event, wrappedHandler)\n\n    // Create cleanup function\n    const cleanup = () => element.removeEventListener(event, wrappedHandler)\n    this.eventListeners.push(cleanup)\n\n    return cleanup\n  }\n\n  /**\n   * Cleanup all performance monitoring resources\n   */\n  destroy(): void {\n    // Disconnect all performance observers\n    this.observers.forEach(observer => observer.disconnect())\n    this.observers.clear()\n\n    // Clear all memory intervals\n    this.memoryIntervals.forEach(interval => clearInterval(interval))\n    this.memoryIntervals.clear()\n\n    // Remove all event listeners\n    this.eventListeners.forEach(cleanup => cleanup())\n    this.eventListeners = []\n\n    // Clear ongoing operations\n    this.ongoingOperations.clear()\n\n    // Clear memory history\n    this.memoryUsageHistory = []\n  }\n\n  /**\n   * Get current memory usage info if available\n   */\n  getMemoryInfo(): { used: number; total: number; limit: number } | null {\n    if ('memory' in performance) {\n      const {memory} = (performance as any)\n      return {\n        used: memory.usedJSHeapSize,\n        total: memory.totalJSHeapSize,\n        limit: memory.jsHeapSizeLimit\n      }\n    }\n    return null\n  }\n\n  /**\n   * Get performance statistics\n   */\n  getStats(): Record<string, any> {\n    const ongoingOps = Array.from(this.ongoingOperations.keys())\n    const memoryInfo = this.getMemoryInfo()\n\n    return {\n      ongoingOperations: ongoingOps.length,\n      memoryInfo,\n      historySize: this.memoryUsageHistory.length,\n      observers: this.observers.size,\n      intervals: this.memoryIntervals.size,\n      eventListeners: this.eventListeners.length\n    }\n  }\n}\n\n// Singleton instance\nexport const performanceMonitor = EnhancedPerformanceMonitor.getInstance()","usedDeprecatedRules":[{"ruleId":"no-return-await","replacedBy":[],"info":{"message":"The original assumption of the rule no longer holds true because of engine optimization.","deprecatedSince":"8.46.0","availableUntil":null,"replacedBy":[]}}]},{"filePath":"/Users/mymac/Projects/HeyTrack/src/utils/performance/performance.ts","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[{"ruleId":"no-return-await","replacedBy":[],"info":{"message":"The original assumption of the rule no longer holds true because of engine optimization.","deprecatedSince":"8.46.0","availableUntil":null,"replacedBy":[]}}]},{"filePath":"/Users/mymac/Projects/HeyTrack/src/utils/performance/usePerformance.ts","messages":[{"ruleId":"react-hooks/exhaustive-deps","severity":2,"message":"React Hook useEffect has a missing dependency: 'renderCount'. Either include it or remove the dependency array.","line":42,"column":6,"nodeType":"ArrayExpression","endLine":42,"endColumn":21,"suggestions":[{"desc":"Update the dependencies array to be: [componentName, renderCount]","fix":{"range":[1442,1457],"text":"[componentName, renderCount]"}}]}],"suppressedMessages":[],"errorCount":1,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"source":"import { useEffect, useRef, useState } from 'react'\nimport { createClientLogger } from '@/lib/client-logger'\nimport { performanceMonitor } from './performance-monitoring'\n\n// Performance monitoring hooks for React components with enhanced memory leak prevention\n\nconst perfLogger = createClientLogger('Performance')\n\n// Hook to measure component render performance with memory leak detection\nexport const useRenderPerformance = (componentName: string) => {\n  const renderStartRef = useRef<number | null>(null)\n  const mountedRef = useRef(true)\n  const [renderCount, setRenderCount] = useState(0)\n\n  useEffect(() => {\n    mountedRef.current = true\n    setRenderCount(prev => prev + 1)\n    \n    // Measure initial render\n    renderStartRef.current = performance.now()\n    \n    // Log when component mounts\n    perfLogger.debug({ componentName, renderCount: 1 }, 'Component mounted')\n    \n    return () => {\n      mountedRef.current = false\n      \n      // Component unmount cleanup\n      perfLogger.debug({ componentName, renderCount }, 'Component unmounted')\n      \n      // Clean up any pending operations\n      if (renderStartRef.current !== null) {\n        const duration = performance.now() - renderStartRef.current\n        perfLogger.warn({ \n          componentName, \n          duration: duration.toFixed(2),\n          renderCount\n        }, 'Pending render operation cleaned up')\n        renderStartRef.current = null\n      }\n    }\n  }, [componentName])\n\n  useEffect(() => {\n    if (!mountedRef.current) {return} // Don't run if component is unmounted\n    \n    if (renderStartRef.current !== null) {\n      const renderTime = performance.now() - renderStartRef.current\n      \n      // Log slow renders\n      if (renderTime > 16.67) { // More than one frame at 60fps\n        perfLogger.warn({ \n          componentName, \n          renderTime: renderTime.toFixed(2),\n          renderCount\n        }, 'Slow component render detected')\n      } else {\n        perfLogger.debug({ \n          componentName, \n          renderTime: renderTime.toFixed(2),\n          renderCount\n        }, 'Component rendered')\n      }\n      \n      renderStartRef.current = null\n    }\n  }, [componentName, renderCount])\n  \n  // Return render count for optimization decisions\n  return { renderCount }\n}\n\n// Hook to measure function execution performance with enhanced tracking\nexport const useFunctionPerformance = (context?: Record<string, unknown>) => {\n  const mountedRef = useRef(true)\n\n  useEffect(() => () => {\n      mountedRef.current = false\n    }, [])\n\n  const measure = async <T>(name: string, fn: () => Promise<T>): Promise<T> => {\n    if (!mountedRef.current) {return fn()} // Execute function directly if component is unmounted\n    \n    const operationId = `${name}-${Date.now()}`\n    performanceMonitor.startOperation(operationId)\n    \n    const start = performance.now()\n    try {\n      const result = await fn()\n      const duration = performance.now() - start\n      \n      performanceMonitor.endOperation(operationId, { \n        ...context,\n        duration: duration.toFixed(2)\n      })\n      \n      if (duration > 100) {\n        perfLogger.warn({ \n          name, \n          duration: duration.toFixed(2),\n          ...context\n        }, 'Slow function execution detected')\n      } else {\n        perfLogger.debug({ \n          name, \n          duration: duration.toFixed(2),\n          ...context\n        }, 'Function executed')\n      }\n      \n      return result\n    } catch (error) {\n      const duration = performance.now() - start\n      \n      performanceMonitor.endOperation(operationId, { \n        ...context,\n        duration: duration.toFixed(2),\n        error: (error as Error).message\n      })\n      \n      perfLogger.error({ \n        name, \n        duration: duration.toFixed(2),\n        ...context,\n        error \n      }, 'Function execution failed')\n      throw error\n    }\n  }\n\n  return { measure }\n}\n\n// Hook to track expensive renders and memoize components with memory leak prevention\nexport const useExpensiveRender = (\n  dependencies: unknown[], \n  thresholdMs = 5\n) => {\n  const prevDepsRef = useRef<unknown[]>([])\n  const renderStartRef = useRef<number>(0)\n  const [totalRenderTime, setTotalRenderTime] = useState(0)\n  const mountedRef = useRef(true)\n\n  useEffect(() => () => {\n      mountedRef.current = false\n    }, [])\n\n  useEffect(() => {\n    if (!mountedRef.current) {return}\n    \n    renderStartRef.current = performance.now()\n    \n    // Check if dependencies changed compared to previous render\n    const depsChanged = dependencies.some((dep, index) => \n      dep !== prevDepsRef.current[index]\n    )\n    \n    prevDepsRef.current = [...dependencies]\n    \n    if (depsChanged) {\n      const renderTime = performance.now() - renderStartRef.current\n      \n      // Update total render time\n      setTotalRenderTime(prev => prev + renderTime)\n      \n      if (renderTime > thresholdMs) {\n        perfLogger.warn({ \n          renderTime: renderTime.toFixed(2), \n          dependenciesCount: dependencies.length,\n          totalRenderTime: totalRenderTime + renderTime\n        }, 'Expensive render with dependencies change')\n      }\n    }\n  }, [dependencies, thresholdMs, totalRenderTime])\n\n  return { totalRenderTime }\n}","usedDeprecatedRules":[{"ruleId":"no-return-await","replacedBy":[],"info":{"message":"The original assumption of the rule no longer holds true because of engine optimization.","deprecatedSince":"8.46.0","availableUntil":null,"replacedBy":[]}}]},{"filePath":"/Users/mymac/Projects/HeyTrack/src/utils/performance/usePerformanceMonitoring.ts","messages":[{"ruleId":"@typescript-eslint/no-unused-vars","severity":2,"message":"'PerformanceLayoutShift' is defined but never used. Allowed unused vars must match /^_/u.","line":34,"column":11,"nodeType":null,"messageId":"unusedVar","endLine":34,"endColumn":33},{"ruleId":"react-hooks/exhaustive-deps","severity":2,"message":"React Hook useEffect has a missing dependency: 'cleanupAll'. Either include it or remove the dependency array.","line":100,"column":6,"nodeType":"ArrayExpression","endLine":100,"endColumn":8,"suggestions":[{"desc":"Update the dependencies array to be: [cleanupAll]","fix":{"range":[2602,2604],"text":"[cleanupAll]"}}]},{"ruleId":"@typescript-eslint/no-explicit-any","severity":2,"message":"Unexpec