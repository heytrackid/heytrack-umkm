  "private": true,
  "type": "module",
  "description": "HeyTrack - Comprehensive UMKM Culinary Business Management System with AI Assistant",
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=6144' next dev --turbo",
    "dev:webpack": "NODE_OPTIONS='--max-old-space-size=6144' NEXT_WEBPACK=1 next dev",
    "dev:clean": "rm -rf .next .turbo node_modules/.cache && NODE_OPTIONS='--max-old-space-size=6144' next dev --turbo",
    "dev:verbose": "NODE_OPTIONS='--trace-warnings --max-old-space-size=6144' next dev --turbo",
    "build": "next build",
    "build:fast": "next build",
    "build:validate": "pnpm run validate && next build",
    "build:parallel": "pnpm run type-check & pnpm run lint:fast & wait && pnpm run build",
    "build:analyze": "ANALYZE=true next build",
    "build:monitor": "node scripts/bundle-monitor.ts",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:fast": "eslint .",
    "lint:quick": "eslint .",
    "lint:all": "eslint .",
     "type-check": "tsc --noEmit",
     "type-check:all": "tsc --noEmit",
     "validate": "pnpm run type-check && pnpm run lint",
     "validate:all": "pnpm run type-check:all && pnpm run lint:all",
     "validate:quick": "pnpm run type-check && pnpm run lint:quick",
    "clean": "rm -rf .next .turbo node_modules/.cache",
    "clean:all": "rm -rf .next .turbo node_modules pnpm-lock.yaml",
    "clean:cache": "rm -rf .next/cache",
    "fix:hmr": "./scripts/fix-hmr.sh",
    "supabase:types": "supabase gen types typescript --local > src/types/supabase-generated.ts",
    "supabase:types:remote": "supabase gen types typescript --project-id vrrjoswzmlhkmmcfhicw > src/types/supabase-generated.ts"
  },
