{
  "name": "heytrack",
  "version": "1.0.0",
  "description": "HeyTrack - Comprehensive Business Management System for BISMILLAH",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:5173": "vite --port 5173",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:debug": "tsc && vite build --minify false",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "test": "echo \"Testing not configured yet\" && exit 0"
  },
  "keywords": [
    "business-management",
    "inventory",
    "orders",
    "customers",
    "suppliers",
    "recipes",
    "financial",
    "react",
    "typescript",
    "vite",
    "supabase"
  ],
  "author": "BISMILLAH Team",
  "license": "MIT",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.8.4",
    "lucide-react": "^0.446.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-accordion": "^1.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "react-hook-form": "^7.53.0",
    "@hookform/resolvers": "^3.9.0",
    "zod": "^3.23.8",
    "date-fns": "^4.1.0",
    "recharts": "^2.12.7",
    "react-i18next": "^15.0.2",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^8.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^22.6.1",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vite-plugin-remove-console": "^2.2.0",
    "rollup-plugin-visualizer": "^5.12.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.10.0",
  "repository": {
    "type": "git",
