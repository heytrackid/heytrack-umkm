{
  "redirects": [
    {
      "source": "/auth/login",
      "destination": "/handler/sign-in",
      "permanent": true
    },
    {
      "source": "/auth/register",
      "destination": "/handler/sign-up",
      "permanent": true
    },
    {
      "source": "/auth/reset-password",
      "destination": "/handler/forgot-password",
      "permanent": true
    },
    {
      "source": "/auth/:path*",
      "destination": "/handler/:path*",
      "permanent": true
    }
  ]
}